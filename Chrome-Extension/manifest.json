{
    "manifest_version": 3,
    "name": "Phishing Website Detector",
    "version": "1.0",
    "description": "Detects phishing websites using machine learning",
    "permissions": ["activeTab", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png" 
    },
    "background": {
      "service_worker": "background.js"
    }
  }